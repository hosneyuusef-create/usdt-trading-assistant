{
  "properties": {
    "price_ceiling": {
      "anyOf": [
        {
          "exclusiveMinimum": 0.0,
          "type": "number"
        },
        {
          "pattern": "^(?!^[-+.]*$)[+-]?0*(?:\\d{0,10}|(?=[\\d.]{1,13}0*$)\\d{0,10}\\.\\d{0,2}0*$)",
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Price Ceiling"
    },
    "split_allowed": {
      "default": false,
      "title": "Split Allowed",
      "type": "boolean"
    },
    "specific_providers": {
      "anyOf": [
        {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Specific Providers"
    },
    "min_fill_percentage": {
      "anyOf": [
        {
          "exclusiveMinimum": 0.0,
          "maximum": 100.0,
          "type": "number"
        },
        {
          "pattern": "^(?!^[-+.]*$)[+-]?0*(?:\\d{0,3}|(?=[\\d.]{1,6}0*$)\\d{0,3}\\.\\d{0,2}0*$)",
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Min Fill Percentage"
    }
  },
  "title": "SpecialConditions",
  "type": "object"
}